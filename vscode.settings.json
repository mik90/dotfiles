{
    "editor.lineNumbers": "relative",
    "window.titleBarStyle": "custom",
    "git.autofetch": true,
    "workbench.colorTheme": "Material Theme Darker High Contrast",
    "workbench.iconTheme": "vscode-icons",
    "workbench.colorCustomizations": {
        "statusBar.background": "#000000",
        "statusBar.foreground": "#ffffff",
        "editorLineNumber.foreground": "#a3a3a3",
        "[Material Theme Darker High Contrast]": {
            "activityBar.background": "#242424",
            "activityBar.foreground": "#ffffff",
            "gitlens.trailingLineForegroundColor": "#5a5a5a",
        },
        "[One Monokai]": {
            "statusBar.background": "#000000",
            "statusBar.foreground": "#ffffff",
            "editorLineNumber.foreground": "#a3a3a3",
        }
    },
    "editor.tokenColorCustomizations": {
        "[Material Theme Darker High Contrast]": {
            "comments": "#00eb0c",
            "textMateRules": [
                {
                    "scope": "string.quoted.docstring.multi.python",
                    "settings": {
                        "foreground": "#00d0df",
                    }
                }
            ]
        },
        "[One Monokai]": {
            "comments": "#00eb0c"
        }
    },
    "editor.defaultFormatter": "matklad.rust-analyzer",
    "editor.renderWhitespace": "all",
    "python.defaultInterpreterPath": "/usr/bin/python3",
    "cmake.configureEnvironment": {
        "PATH": "$PATH:/home/mike/.local/bin"
    },
    "cmake.parallelJobs": 24,
    "update.mode": "manual",
    "explorer.confirmDragAndDrop": false,
    "editor.formatOnSave": true,
    "discord.silent": true,
    "editor.tabSize": 2,
    "clang-format.language.proto.enable": false,
    "git.suggestSmartCommit": false,
    "vsicons.dontShowNewVersionMessage": true,
    "clang-tidy.executable": "clang-tidy",
    "rust.jobs": 12,
    "window.restoreWindows": "none",
    "window.zoomLevel": 2,
    "remote.extensionKind": {
        "ms-azuretools.vscode-docker": "workspace"
    },
    "clang-tidy.blacklist": [
        ".*pb.h"
    ],
    "rust.rustfmt_path": "/home/mike/.cargo/bin/rustfmt",
    "rust-client.rlsPath": "/home/mike/.cargo/bin/rls",
    "rust-client.revealOutputChannelOn": "error",
    "files.watcherExclude": {
        "**/target/**": true
    },
    "explorer.confirmDelete": false,
    "rust-client.autoStartRls": false,
    "rust.clippy_preference": "on",
    "[python]": {
        "editor.defaultFormatter": "ms-python.python"
    }
}